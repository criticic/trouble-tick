{
    "name": "trouble-tick",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "next dev --turbopack",
        "build": "next build",
        "start": "next start",
        "lint": "next lint",
        "db:generate": "drizzle-kit generate",
        "db:migrate": "drizzle-kit migrate",
        "db:studio": "drizzle-kit studio",
        "db:start": "turso dev --db-file=./src/lib/db/local.sqlite",
        "db:clear": "rm ./src/lib/db/local.sqlite*",
        "db:clear-migrations": "rm -rf ./src/lib/db/migrations",
        "format": "prettier --check .",
        "format:fix": "prettier --write .",
        "preload": "node -e \"import('geolite2-redist').then(geolite => geolite.downloadDbs())\""
    },
    "dependencies": {
        "@auth/drizzle-adapter": "^1.7.4",
        "@libsql/client": "^0.14.0",
        "@radix-ui/react-dialog": "^1.1.5",
