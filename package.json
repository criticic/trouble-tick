    "devDependencies": {
        "@eslint/eslintrc": "^3",
        "@trivago/prettier-plugin-sort-imports": "^5.2.1",
        "@types/lodash": "^4.17.14",
        "@types/node": "^20",
        "@types/react": "^19",
        "@types/react-dom": "^19",
        "drizzle-kit": "^0.30.3",
        "eslint": "^9",
        "eslint-config-next": "15.1.6",
        "eslint-config-prettier": "^10.0.1",
        "postcss": "^8",
